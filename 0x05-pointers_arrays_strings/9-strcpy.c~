#include "holberton.h"
/**
 * _strcpy - copies a string
 * @dest: pointer to destination
 * @src: pointer to source
 * Return: pointer
 */
char *_strcpy(char *dest, char *src)
{
  int i, j;
  for (i = 0; *(src + i) != '\0'; i++)
      {
	*(dest + i) = *(src + i);
}
    *(dest + j) = *(src + i);
return (dest);
}
